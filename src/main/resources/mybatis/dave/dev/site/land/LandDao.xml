<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="landDAO">

    <select id="selectLand" resultType="LandVO" parameterType="LandVO">
        /* landDAO.selectLand */
        <![CDATA[
        SELECT ID
              , ADDRESS
              , AREA
        FROM    LANDS
        WHERE   ID = #{id}
        ]]>
    </select>

    <select id="selectLands" resultType="LandVO" parameterType="LandVO">
        /* landDAO.selectLand */
        <![CDATA[
        SELECT ID
              , ADDRESS
              , AREA
        FROM    LANDS
        WHERE   ID = #{id}
        ]]>
    </select>

</mapper>
